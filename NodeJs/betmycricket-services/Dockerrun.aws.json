{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "server",
      "image": "santhu538/betmycricket",
      "hostname": "api",
      "essential": false,
      "memory": 512
    },
    {
      "name": "nginx",
      "image": "santhu538/betmycricket-proxy",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["server"],
      "memory": 256
    }
  ]
}
